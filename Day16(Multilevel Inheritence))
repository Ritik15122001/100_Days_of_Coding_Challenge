package com.company;
class baseA{
    public baseA() {
        System.out.println("I am a constructor of baseA");
    }
}
class DerivedA extends baseA{
    public DerivedA() {
        System.out.println("I am a Constructor of DerivedA");
    }
}
class DerivedB extends DerivedA{
    public DerivedB() {
        System.out.println("I am a constructor of DerivedB");
    }
}
public class practice115 {
    public static void main(String[] args) {
       DerivedB obj = new DerivedB();

    }
}
