package com.company;
interface college{
     public void department();
}
interface  student{
    public void studentamt();
}
class BCA implements college,student{
   public  void department()
    {
        System.out.println("BCA");
    }
    public void studentamt()
    {
        System.out.println("120");
    }
    void teachers()
    {
        System.out.println("5");
    }
}
public class practice117 {
    public static void main(String[] args) {
        BCA stu = new BCA();
        stu.department();
        stu.teachers();
        stu.studentamt();

    }
}
