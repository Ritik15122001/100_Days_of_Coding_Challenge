package com.company;
class base1{
    int x;
    public base1() {
        super();
        System.out.println("i am the base constructor");
    }
    public base1(int x) {
        System.out.println("i am the base over loadded constructor"+x);
    }
}
class derived1 extends base1{
    int d;
    public derived1() {
        //super(0);
        System.out.println("I am the derived constructor");
    }
    public derived1(int x,int y) {
        super(x);
        System.out.println("I am the derived  with "+x+" with "+y);

    }

}
public class practice99 {
    public static void main(String[] args) {
        //base1 b = new base1();
        //derived1 d = new derived1();
        derived1 d1 = new derived1(14,3);

    }
}
