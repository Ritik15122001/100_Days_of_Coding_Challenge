import java.util.Scanner;
public class practice47 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("enter the number:");
        int n = sc.nextInt();
        int cout=0;
        do {
            n=n/10;
            cout++;
        }while (n!=0);
        System.out.println(cout);
       
    }
}
